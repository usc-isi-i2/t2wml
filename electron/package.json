    "tree-kill": "^1.2.2",
    "ts-loader": "^8.0.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "yarn": "^1.22.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usc-isi-i2/t2wml"
  },
  "bugs": {
    "url": "https://github.com/usc-isi-i2/t2wml/issues"
  },
  "homepage": "https://github.com/usc-isi-i2/t2wml/issues#readme"
}
