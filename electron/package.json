    "eslint-watch": "^7.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "js-file-download": "^0.4.11",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.21",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "monaco-languages": "^1.10.0",
